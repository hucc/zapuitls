<?xml version="1.0" encoding="UTF-8"?>
<Loggers>
    <Logger name="Logger">
        <!--日志文件路径-->
        <FilePath>./logs/fundqin.log</FilePath>
        <!--日志级别-->
        <Level>DebugLevel</Level>
        <!--每个日志文件保存的最大尺寸 单位：M-->
        <MaxSize>128</MaxSize>
        <!--日志文件最多保存多少个备份-->
        <MaxBackups>30</MaxBackups>
        <!--文件最多保存多少天-->
        <MaxAge>7</MaxAge>
        <!--是否压缩-->
        <Compress>true</Compress>
    </Logger>

</Loggers>


